{
    "manifest_version": 2,
    "name": "FlipTime",
    "description": "A simple flip clock new tab extension",
    "version": "1.1.0",
    "icons": {
        "128": "assets/img/icon128.png"
    },
    "browser_action": {
        "default_icon": "assets/img/icon.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "assets/js/jquery-slim.min.js",
            "assets/js/flipclock.js",
            "assets/js/moment.min.js",
            "assets/js/main.js"
        ]
    }
}