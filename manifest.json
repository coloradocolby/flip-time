{
  "manifest_version": 2,
  "name": "flip-time",
  "description": "minimalist flip clock displays time on each new tab",
  "version": "1.3.4",
  "icons": {
    "128": "assets/img/icon128.png"
  },
  "browser_action": {
    "default_icon": "assets/img/icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["tabs", "storage"]
}
